
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model IEnumerable<Article>
<h1>Delete</h1>

<div>

    @if (Model.Any())
    {

        @foreach (var x in Model.OrderBy(x => x.PublishDate))
        {

    <div>
        <button><a asp-controller="Article" asp-action="Delete" asp-route-Id=@x.Id>Delete</a></button>@x.Title
    </div>


        }

    }
    else
    {
        <textarea>There is no aticles!</textarea>
    }

</div>


